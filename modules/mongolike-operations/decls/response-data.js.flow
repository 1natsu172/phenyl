// @flow
import type {
  Restorable,
} from './restorable.js.flow'

import type {
  ErrorResult,
} from './error-result.js.flow'

import type {
  CustomQueryResult,
  QueryResult,
  SingleQueryResult,
} from './query-result.js.flow'

import type {
  CommandResult,
  GetCommandResult,
  MultiValuesCommandResult,
  CustomCommandResult,
  LoginCommandResult,
  LogoutCommandResult,
} from './command-result.js.flow'

export type ResponseData =
  FindResponseData |
  FindOneResponseData |
  GetResponseData |
  GetByIdsResponseData |
  InsertResponseData |
  InsertAndGetResponseData |
  InsertAndGetMultiResponseData |
  UpdateResponseData |
  UpdateAndGetResponseData |
  UpdateAndFetchResponseData |
  DeleteResponseData |
  RunCustomQueryResponseData |
  RunCustomCommandResponseData |
  LoginResponseData |
  LogoutResponseData |
  ErrorResponseData

export type FindResponseData = {|
  +find: QueryResult,
|}

export type FindOneResponseData = {|
  +findOne: SingleQueryResult,
|}

export type GetResponseData = {|
  +get: SingleQueryResult,
|}

export type GetByIdsResponseData = {|
  +getByIds: QueryResult,
|}

export type InsertResponseData = {|
  +insert: CommandResult,
|}

export type InsertAndGetResponseData = {|
  +insertAndGet: GetCommandResult,
|}

export type InsertAndGetMultiResponseData = {|
  +insertAndGetMulti: MultiValuesCommandResult,
|}

export type UpdateResponseData = {|
  +update: CommandResult,
|}

export type UpdateAndGetResponseData = {|
  +updateAndGet: GetCommandResult,
|}

export type UpdateAndFetchResponseData = {|
  +updateAndFetch: MultiValuesCommandResult,
|}

export type DeleteResponseData = {|
  +delete: CommandResult,
|}

export type RunCustomQueryResponseData = {|
  +runCustomQuery: CustomQueryResult,
|}

export type RunCustomCommandResponseData = {|
  +runCustomCommand: CustomCommandResult,
|}

export type LoginResponseData = {|
  +login: LoginCommandResult,
|}

export type LogoutResponseData = {|
  +logout: LogoutCommandResult,
|}


export type ErrorResponseData = {|
  +error: ErrorResult,
|}
