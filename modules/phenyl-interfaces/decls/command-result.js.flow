// @flow

import type { Restorable } from './restorable.js.flow'
import type { Session } from './session.js.flow'
import type { RestorableEntity } from './restorable.js.flow'

export type CommandResult = {
  ok: 1,
  n: number,
}

export type FetchCommandResult = {
  ok: 1,
  n: number,
  values: Array<Restorable>
}

export type GetCommandResult = {
  ok: 1,
  n: number,
  value: Restorable
}

export type CustomCommandResult = {
  ok: 1,
  result?: Object
}

export type LoginCommandResult = {
  ok: 1,
  session: Session,
  user: ?RestorableEntity,
}

export type LogoutCommandResult = {
  ok: 1,
}
